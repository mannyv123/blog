  "sideEffects": false,
  "scripts": {
    "prepare": "husky install || true",
    "test": "echo I should write some tests ðŸ˜…",
    "prebuild": "npm run clean && echo All clean âœ¨",
    "build": "npm run build:css:prod && remix build",
    "css:watch": "npm run build:css -- --w",
    "clean": "rimraf ./node_modules/.cache ./server/dist ./build ./public/build \"./app/styles/**/*.css\"",
    "deploy": "fly deploy --remote-only",
    "typecheck": "tsc -b .",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "npm run build:css -- --env production",
    "dev": "concurrently \"npm run css:watch\" \"remix dev\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|vue)\"",
    "start": "npm run build:css:prod && remix-serve build",
    "validate": "./other/validate",
    "start:local": "npm run build:css:prod && dotenv -e .env remix-serve build"
  },
