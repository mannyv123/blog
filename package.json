{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css:prod && remix build",
    "css:watch": "npm run build:css -- --w",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "npm run build:css -- --env production",
    "dev": "concurrently \"npm run css:watch\" \"cross-env NODE_ENV=development netlify dev\"",
    "postinstall": "remix setup node",
    "start": "npm run build:css:prod && cross-env NODE_ENV=production netlify dev"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@netlify/functions": "^0.10.0",
    "@octokit/plugin-throttling": "^3.6.2",
    "@octokit/rest": "^18.12.0",
    "@remix-run/netlify": "^1.2.2",
    "@remix-run/react": "^1.2.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "clsx": "^1.1.1",
    "hast-util-select": "^5.0.2",
    "mdx-bundler": "^9.0.1",
    "p-queue": "^7.2.0",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reading-time": "^1.5.0",
    "rehype-add-classes": "^1.0.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-code-titles": "^1.1.0",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.1.0",
    "remark-autolink-headings": "^7.0.1",
    "remark-capitalize": "^1.1.0",
    "remark-code-titles": "^0.1.2",
    "remark-emoji": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-images": "^3.1.0",
    "remark-slug": "^7.0.1",
    "remark-smartypants": "^2.0.0",
    "remix": "^1.2.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
