{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css:prod && remix build",
    "css:watch": "npm run build:css -- --w",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "npm run build:css -- --env production",
    "dev": "concurrently \"npm run css:watch\" \"cross-env NODE_ENV=development netlify dev\"",
    "postinstall": "remix setup node",
    "start": "npm run build:css:prod && cross-env NODE_ENV=production netlify dev"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.2.2",
    "@remix-run/react": "^1.2.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "clsx": "^1.1.1",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
